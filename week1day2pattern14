#include <stdio.h>
#include <stdlib.h>
int main() {
    int rows=5;
    for (int i = 1; i <= rows; i++) {
        for (int j = 1; j <= i; j++) {
            printf("%d ", j);
        }
        
        for (int k = i - 1; k >= 1; k--) {
            printf("%d ", k);
        }
        printf("\n");
    }

    return 0;
}

